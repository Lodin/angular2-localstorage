{
  "name": "angular2-localstorage",
  "version": "1.0.0-alpha.0",
  "description": "Angular2 decorator to save and restore class properties automatically from any WebStorage.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run tsc && npm run remove:specs",
    "tsc": "tsc -p src",
    "remove:specs": "rimraf \"dist/**/*.spec.js\" \"dist/**/*.spec.js.map\" \"dist/**/*.spec.d.ts\"",
    "prepublish": "npm run build",
    "test": "karma start config/karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcj/angular2-localstorage.git"
  },
  "keywords": [
    "Angular2",
    "LocalStorage"
  ],
  "author": "Marc J. Schmidt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcj/angular2-localstorage/issues"
  },
  "homepage": "https://github.com/marcj/angular2-localstorage#readme",
  "dependencies": {
    "assign-deep": "^0.4.5",
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0",
    "ts-helpers": "^1.1.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/compiler-cli": "^0.6.3",
    "@angular/core": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "@angular/platform-browser-dynamic": "2.0.1",
    "@angular/platform-server": "2.0.1",
    "@types/deep-equal": "0.0.30",
    "@types/jasmine": "^2.2.34",
    "awesome-typescript-loader": "^2.2.4",
    "codecov": "^1.0.1",
    "deep-equal": "^1.0.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-remap-istanbul": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
